{
    "name": "kinatechsolutions/bug-clasper",
    "description": "A easy way to report Exceptions from a laravel app to various channels including Slack, Email, Sms",
    "type": "library",
    "authors": [
        {
            "name": "Kinare",
            "email": "michaelkinare@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/slack-notification-channel": "^2.4",
        "africastalking/africastalking": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kinatech\\BugClasper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kinatech\\BugClasper\\Providers\\BugClasperServiceProvider"
            ]
        }
    }
}
